<li class="session-action"><%= link_to 'Logout', session_path, :method => :delete %></li>
<li class="session-action" style="margin-left: 20px;"><%= link_to session[:user_login], user_path(session[:user_id]) %></li>
<% else -%>
<li class="session-action"><%= link_to 'Signup', new_user_path %></li>
